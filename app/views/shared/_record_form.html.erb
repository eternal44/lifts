<%= form_for(@record) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div>
    <%= f.collection_select :lift_id, Lift.all, :id, :lift_name %>
  </div>
  <br>
  <div class="field">
    <%= f.label :lift_weight %><br>
    <%= f.number_field :lift_weight %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
